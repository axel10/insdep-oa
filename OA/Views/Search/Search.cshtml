@model dynamic

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
    <script src="/Scripts/js/vender/jquery-1.10.2.js"></script>
</head>
<body>
<div>
    <input type="text" id="kw" />
    <input type="button" value="提交" id="submit"/>
</div>

<script>
    $("#submit").on("click",
        function() {
            var kw = $("#kw").val();
            $.ajax({
                url: "/search/GetData?kw=" + kw,
                success: function(data) {
                    console.log(JSON.parse(data));
                }
            });
        })
</script>
</body>
</html>